group("maxwell") {
  testonly = true

  deps = [
    "acquirers",
    "agents",
    "context_engine",
    # "integration",
    "suggestion_engine",
  ]
}

source_set("bound_set") {
  sources = [
    "bound_set.h",
  ]

  deps = [
    "//mojo/public/cpp/bindings:core",
  ]
}

source_set("debug") {
  sources = [
    "debug.cc",
    "debug.h",
  ]

  deps = [
    "//apps/maxwell/interfaces",
    "//lib/mtl:mtl_with_mojo",
    "//mojo/public/cpp/application",
    "//mojo/public/cpp/bindings",
    "//mojo/public/cpp/environment",
  ]
}
