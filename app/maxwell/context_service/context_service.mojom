module intelligence;

[ServiceName="intelligence.ContextService"]
interface ContextPublisher {
  StartPublishing(string whoami, PublisherPipe& pipe);
};

interface PublisherPipe {
  Publish(string label, string? value) => (Status status);
};

enum Status { Ok };
