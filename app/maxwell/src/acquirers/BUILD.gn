group("acquirers") {
  deps = [
    ":gps",
  ]
}

executable("focus") {
  output_name = "acquirers/focus"

  sources = [
    "focus.cc",
    "focus.h",
  ]

  deps = [
    "//apps/maxwell/services/context",
    "//apps/modular/lib/app",
    "//apps/modular/services/user",
    "//lib/mtl",
  ]
}

executable("gps") {
  output_name = "acquirers/gps"

  sources = [
    "gps.cc",
    "gps.h",
  ]

  deps = [
    "//apps/maxwell/services/context",
    "//apps/modular/lib/app",
    "//lib/mtl",
  ]
}
