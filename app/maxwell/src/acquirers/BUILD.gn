group("acquirers") {
  deps = [
    ":gps",
    ":modular_acquirer",
  ]
}

executable("gps") {
  output_name = "acquirers/gps"

  sources = [
    "gps.cc",
    "gps.h",
  ]

  deps = [
    "//apps/maxwell/services/context",
    "//apps/modular/lib/app",
    "//lib/mtl",
  ]
}

executable("modular_acquirer") {
  output_name = "acquirers/modular_acquirer"

  sources = [
    "modular_acquirer.cc",
    "modular_acquirer.h",
  ]

  deps = [
    "//apps/maxwell/services/context",
    "//apps/modular/lib/app",
    "//lib/mtl",
  ]
}
