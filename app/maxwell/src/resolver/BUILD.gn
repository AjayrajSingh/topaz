group("resolver") {
  deps = [
    ":resolver_main",
  ]
}

executable("resolver_main") {
  sources = [
    "resolver_impl.cc",
    "resolver_impl.h",
    "resolver_main.cc",
  ]

  deps = [
    "//apps/component_manager/services",
    "//apps/maxwell/services/resolver",
    "//apps/modular/lib/app",
    "//apps/modular/services/application:application_sync",
    "//apps/modular/services/document_store",
    "//lib/ftl",
    "//lib/mtl",
    "//third_party/rapidjson",
  ]
}
