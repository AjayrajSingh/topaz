import("//lib/fidl/fidl.gni")

fidl("context") {
  sources = [
    "context_engine.fidl",
    "context_reader.fidl",
    "context_publisher.fidl",
    "metadata.fidl",
  ]

  deps = [
    "//apps/maxwell/services/user:scope",
