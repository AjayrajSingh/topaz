library fuchsia.textinputmod;

[Discoverable]
interface TextInputModReceiver {
    UserEnteredText(string text) -> ();
    UserCanceled() -> ();
};

[Discoverable]
interface TextInputMod {
    ListenForTextInput(TextInputModReceiver receiver) -> ();
};
